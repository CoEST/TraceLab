<?xml version="1.0" encoding="utf-8" ?>
<artifacts_collection>
<!-- This is example how coest format allows loading artifact content from external relative location - the content becomes the path -->
<collection_info>
    <id>Sample Collection 2</id>
    <name>Name of the collection</name>
    <version>1.1</version>
    <description>This is sample coest format with external location</description>
	<content_location>external</content_location><!-- Note, that content location has been set to external -->
</collection_info>
<artifacts>
	<artifact><id>EA140</id><content>External/EA140.txt</content><parent_id/></artifact>
	<artifact><id>EA141</id><content>External/EA141.txt</content><parent_id/></artifact>
	<artifact><id>EA142</id><content>External/EA142.txt</content><parent_id/></artifact>
</artifacts>
</artifacts_collection>
